v.Local.dEngineer.Set(v.Screen.[v.Args.Screen]!dtpEngDate.Value)
v.Local.dQA.Set(v.Screen.[v.Args.Screen]!dtpQADate.Value)
F.Intrinsic.Control.If(v.Args.Screen, =, "FORM_ASSEMBLY", "OR", v.Args.Screen, =, "FORM_INSPECTION", v.Args.Screen, =, "FORM_REINFORCEMENT", v.Args.Screen, =, "FORM_TOPCOAT", v.Args.Screen, =, "FORM_TRIMMING")
	v.Local.iDataReqInProc.Set(v.Screen.[v.Args.Screen]!chkReqInProcess.Value)
F.Intrinsic.Control.Else
	v.Local.iDataReqInProc.Set(0)
F.Intrinsic.Control.EndIf
'Save to Table
F.Intrinsic.Control.If(v.Global.iID, !=, 0)
	'Update Record
	F.Intrinsic.String.Build("Update GCG_4586_Router Set Customer = '{0}', CustomerPartNum = '{1}', Part_Desc = '{2}', Product_Line = '{3}', MLCat = '{4}', Code = '{5}'",v.Screen.[v.Args.Screen]!txtCustomer.text, v.Screen.[v.Args.Screen]!txtPart.Text, v.Screen.[v.Args.Screen]!txtDesc.Text, v.Screen.[v.Args.Screen]!txtPL.Text, v.Screen.[v.Args.Screen]!txtMLCat.Text, v.Screen.[v.Args.Screen]!txtCode.Text, v.Local.sSql)
	F.Intrinsic.String.Build("{0}, Wall = '{1}', AWG = '{2}', H_Length = '{3}', DataReqFinal = '{4}'", v.Local.sSql, v.Screen.[v.Args.Screen]!txtWall.Text, v.Screen.[v.Args.Screen]!txtAWG.Text, v.Screen.[v.Args.Screen]!txtLength.Text, v.Screen.[v.Args.Screen]!chkReqFinal.Value, v.Local.sSql)
	F.Intrinsic.String.Build("{0}, FinalInspce = '{1}', Engineer = '{2}', QA = '{3}', EngDate = '{4}'", v.Local.sSql, v.Screen.[v.Args.Screen]!chkFinalInsp.Value, v.Screen.[v.Args.Screen]!txtEngineer.Text, v.Screen.[v.Args.Screen]!txtQA.Text, v.Local.dEngineer.PervasiveDate, v.Local.sSql)
	F.Intrinsic.String.Build("{0}, QADate = '{1}', Header_Notes = '{2}', LHR_NO = '{3}', Procedure_NO = '{4}'", v.Local.sSql, v.Local.dQA.PervasiveDate, v.Screen.[v.Args.Screen]!mltxtNotesHeader.text,v.Screen.Form_Assembly!txtLHR.Text, v.Screen.Form_Assembly!txtProcedure.Text, v.Local.sSql)
	F.Intrinsic.String.Build("{0}, Form_NO = '{1}', DataReqInProc = '{2}', Assy_Inst = '{3}', Inspection_Inst = '{4}'", v.Local.sSql, v.Screen.Form_Assembly!txtForm.text, v.Local.iDataReqInProc.long, v.Screen.Form_Assembly!mltxtAssembly.Text, v.Screen.Form_Inspection!mltxtInspection.Text,v.Local.sSql)
